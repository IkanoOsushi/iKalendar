<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.t_robop.ikalendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ikalendar_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ikalendar_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.ReminderActivity" />
        <activity android:name=".Activity.TimetableActivity" />
        <activity android:name=".Activity.CalendarActivity" />
        <activity android:name=".Activity.ReminderEditActivity" />
        <activity android:name=".Activity.TableTimeTodayActivity" />
        <activity android:name=".Activity.TimetableEditActivity" />
        <activity android:name=".Calender.CalendarDayViewActivity"
            android:parentActivityName=".Activity.CalendarActivity"/>
        <activity android:name=".Activity.CalendarAddPlanActivity"
            android:parentActivityName=".Activity.CalendarActivity"></activity>
    </application>

</manifest>